{
	"arrowParens": "always",
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "strict",
	"jsxBracketSameLine": true,
	"jsxSingleQuote": false,
	"printWidth": 120,
	"proseWrap": "always",
	"quoteProps": "as-needed",
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true
}
